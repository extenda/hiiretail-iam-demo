{
  "name": "token-cache",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@hiiretail/core": "^0.5.5",
    "@hiiretail/synergy-ui": "^0.20.4",
    "@hookform/resolvers": "^2.8.8",
    "@loadable/component": "^5.15.2",
    "axios": "^0.26.0",
    "react": "^17.0.1",
